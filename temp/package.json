{
  "name": "marketing-site",
  "description": "New SSR marketing site",
  "version": "1.0.0",
  "private": true,
  "author": "KL Dev Team",
  "email": "Vitaly.Zaitsev@kaspersky.com",
  "repository": {
    "type": "git",
    "url": "https://hqrndtfs.avp.ru/tfs/DefaultCollection/_git/CIWD-CorpSites"
  },
  "keywords": [
    "gatsby",
    "storybook",
    "typescript"
  ],
  "license": "MIT",
  "main": "n/a",
  "browser": true,
  "scripts": {
    "start": "npm-run-all --parallel start:storybook content start:gatsby",
    "start:storybook": "start-storybook -p 9000 -c .storybook",
    "start:gatsby": "gatsby develop",
    "validate": "npm-run-all --parallel lint test",
    "lint": "eslint \"{src,utils}/**/*.{js,jsx}\"",
    "lint:fix": "eslint --fix \"{src,utils}/**/*.{js,jsx}\"",
    "build": "npm-run-all --sequential validate build:all",
    "build:all": "npm-run-all --parallel build:storybook build:gatsby:prod",
    "build:storybook": "build-storybook -c .storybook -o public/docs",
    "build:gatsby": "gatsby build",
    "build:gatsby:prod": "cross-env mode=prod gatsby build",
    "serve": "npm-run-all --parallel serve:storybook serve:gatsby",
    "serve:storybook": "start-storybook -s ./public/docs -p 9001",
    "serve:gatsby": "gatsby serve -p 8001",
    "content": "npm-run-all --sequential content:prepare content:serve",
    "content:prepare": "node ../../../tools/tridionJsonConverter/start",
    "content:serve": "node utils/static-server",
    "clear": "npm-run-all --sequential clear:gatsby clear:yarn",
    "clear:gatsby": "rimraf .cache && rimraf public",
    "clear:yarn": "rimraf yarn.lock && rimraf node_modules",
    "test": "echo \"Warning: no test specified\" && exit 0",
    "dev": "start-storybook -p 9000 -c .storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.1.0",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint-import-resolver-alias": "^1.1.1",
    "gatsby": "next",
    "gatsby-plugin-manifest": "next",
    "gatsby-plugin-postcss": "next",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-sass": "next",
    "gatsby-plugin-sitemap": "next",
    "gatsby-remark-prettier": "^1.0.0",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-json": "next",
    "gatsby-transformer-remark": "^2.1.6",
    "loaders.css": "^0.1.2",
    "lodash": "^4.17.11",
    "node-sass": "^4.9.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-loaders": "^3.0.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "sass-loader": "^7.1.0",
    "svg-url-loader": "^2.3.2",
    "style-loader": "^0.23.0",
    "webpack": "^4.18.1"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^3.4.11",
    "@storybook/react": "^3.4.11",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-graphql": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "gatsby-plugin-eslint": "^2.0.1",
    "gatsby-plugin-react-svg": "^2.0.0-beta1",
    "husky": "^1.1.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.7.0",
    "react-svg-loader": "^2.1.0",
    "rimraf": "^2.6.2",
    "resolve-url-loader": "^3.0.0",
    "storybook-addon-jsx": "^5.4.0",
    "static-server": "^2.2.1",
    "style-loader": "^0.23.0",
    "svg-inline-loader": "^0.8.0",
    "webpack-merge": "^4.1.4"
  }
}
