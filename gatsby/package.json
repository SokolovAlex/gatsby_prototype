{
  "name": "kl-marketing-editor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "editor": "cd editor && node scripts/start",
    "editor:build": "cd editor && node scripts/build",
    "build": "cd pages && gatsby build",
    "dev": "cd pages && gatsby develop",
    "server": "cd editor && node server/app",
    "move": "node editor/publish",
    "build-pages": "node editor/build-pages",
    "format": "prettier --write '**/*.js'",
    "clear": "rimraf pages/.cache && rimraf pages/public",
    "shared": "node scripts/shared"
  },
  "dependencies": {
    "@koa/cors": "2",
    "@material-ui/core": "^3.1.0",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-named-asset-import": "^0.2.3",
    "babel-preset-react-app": "^5.0.3",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "css-loader": "1.0.0",
    "del": "^3.0.0",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "gatsby": "next",
    "gatsby-plugin-eslint": "^2.0.1",
    "gatsby-plugin-manifest": "next",
    "gatsby-plugin-offline": "next",
    "gatsby-plugin-postcss": "next",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-sass": "next",
    "gatsby-plugin-sitemap": "next",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-json": "next",
    "globby": "^8.0.1",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^4.0.0-beta.4",
    "jest": "20.0.4",
    "jquery": "^3.3.1",
    "koa": "^2.5.3",
    "koa-body": "^4.0.4",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "loaders.css": "^0.1.2",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "node-sass": "^4.9.3",
    "object-assign": "4.1.1",
    "ory-editor-core": "^0.4.4",
    "ory-editor-plugins-background": "^0.4.4",
    "ory-editor-plugins-default-native": "^0.4.4",
    "ory-editor-plugins-divider": "^0.4.4",
    "ory-editor-plugins-image": "^0.4.4",
    "ory-editor-plugins-slate": "^0.4.4",
    "ory-editor-plugins-spacer": "^0.4.4",
    "ory-editor-renderer": "^0.4.4",
    "ory-editor-ui": "^0.4.4",
    "pnp-webpack-plugin": "^1.2.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "prettier": "^1.14.2",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.3.0",
    "react-dev-utils": "^6.1.1",
    "react-dom": "^16.3.0",
    "react-helmet": "^5.2.0",
    "react-jss": "^8.6.1",
    "react-loader-spinner": "^2.2.0",
    "react-loaders": "^3.0.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "resolve": "1.6.0",
    "sass-loader": "7.1.0",
    "static-server": "^2.2.1",
    "style-loader": "0.23.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "^4.18.1",
    "webpack-manifest-plugin": "^2.0.4",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
