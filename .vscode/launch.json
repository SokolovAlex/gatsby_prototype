{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Gatsby develop",
            "request": "launch",
            "type": "node",
            "protocol": "inspector",
            "program": "${workspaceRoot}/gatsby/node_modules/gatsby/dist/bin/gatsby",
            "args": ["develop", "-p", "7777"],
            "stopOnEntry": false,
            "cwd": "${workspaceRoot}/gatsby",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
              "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development",
                "DEBUG": "gatsby:*"
            },
            "console": "integratedTerminal",
            "sourceMaps": true,
            "outFiles": []
        },
        {
            "name": "Gatsby build",
            "request": "launch",
            "type": "node",
            "protocol": "inspector",
            "program": "${workspaceRoot}/gatsby/node_modules/gatsby/dist/bin/gatsby",
            "args": ["build"],
            "stopOnEntry": false,
            "cwd": "${workspaceRoot}/gatsby",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
              "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development",
                "DEBUG": "gatsby:*"
            },
            "console": "integratedTerminal",
            "sourceMaps": true,
            "outFiles": []
        }
    ]
}